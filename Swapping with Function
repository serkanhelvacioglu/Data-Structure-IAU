#include<iostream>

using namespace std;

int swapping(int &x, int &y){
	int z;
	z=x;
	x=y;
	y=z;

	return x,y;
}

int main(){

	int x,y,asd;
	cout<<"Please enter X number: ";
	cin>>x;
	cout<<"Please enter Y number: ";
	cin>>y;

	swapping(x,y);
	cout<<"X swapped Value: "<<x<<endl;
	cout<<"Y swapped Value: "<<y<<endl;

	cin>>asd;

}
